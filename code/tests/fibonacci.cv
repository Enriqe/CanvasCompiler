program hello_world2

function myfunc1 (int arg1) returns int [
    int sol = 1
    if( arg1 == 1)[
        sol = 1
    ]elsif(arg1 == 0)[
        sol = 0
    ]else[
        sol = myfunc1(arg1 - 1) + myfunc1(arg1 - 2)
    ]
    return sol
]

main [
    int myVar = myfunc1(5)
    print(myVar)
]

finish
